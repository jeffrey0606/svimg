{
  "name": "svimg",
  "version": "3.2.0",
  "private": false,
  "description": "Svelte image component with image preprocessing and lazy loading",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Image.svelte": {
      "types": "./dist/Image.d.ts",
      "svelte": "./src/Image.svelte"
    },
    "./s-image": "./dist/s-image.js"
  },
  "svelte": "src/Image.svelte",
  "files": [
    "dist",
    "src/Image.svelte"
  ],
  "typesVersions": {
    ">4.0": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ],
      "Image.svelte": [
        "./dist/Image.d.ts"
      ]
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "watch": "rollup -cw"
  },
  "engines": {
    "pnpm": ">=7.0.0",
    "node": ">=16"
  },
  "keywords": [
    "svelte",
    "image",
    "img",
    "images",
    "lazy",
    "load",
    "responsive",
    "srcset",
    "sharp"
  ],
  "author": "Chris Han <christopher.f.han@gmail.com> (chris-han.com)",
  "homepage": "https://github.com/xiphux/svimg",
  "bugs": {
    "url": "https://github.com/xiphux/svimg/issues",
    "email": "christopher.f.han@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiphux/svimg.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@tsconfig/svelte": "^5.0.2",
    "@types/bluebird": "^3.5.41",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.18.8",
    "@types/sharp": "^0.32.0",
    "jest": "^29.7.0",
    "rollup": "^4.3.0",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "svelte": "^4.2.2",
    "ts-jest": "^29.1.1",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "md5-file": "^5.0.0",
    "node-html-parser": "^6.1.11",
    "p-queue": "^7.4.1",
    "sharp": "^0.32.6",
    "string-replace-async": "^3.0.2"
  }
}
